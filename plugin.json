{
  "id": "nodebb-plugin-ns-utils",
  "name": "NodeBB Utils",
  "description": "Set of utility functions to help you support your forum. Do you need to purge chats? Do you need to delete obsolete data? This plugin will help you.",
  "url": "https://github.com/NicolasSiver/nodebb-plugin-ns-utils",
  "library": "./plugin/index.js",
  "hooks": [
    {
      "hook": "filter:admin.header.build",
      "method": "hooks.filters.adminHeaderBuild"
    },
    {
      "hook": "static:app.load",
      "method": "hooks.statics.load"
    }
  ],
  "less": [],
  "scripts": [],
  "staticDirs": {
    "acp": "./client/acp",
    "css": "./public/css",
    "js": "./public/js"
  },
  "templates": "./public/templates"
}